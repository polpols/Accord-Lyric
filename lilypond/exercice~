staffGuitareJazz = \new Staff {
	\time 4/4
	\set Staff.instrumentName = "Guitare Jazz"
	\set Staff.midiInstrument = "electric guitar (jazz)"
	\transposition c,
	\key a \minor
	\clef treble
	\relative c' { 	
		\context Voice = "melodyGui" {
			\dynamicUp		
